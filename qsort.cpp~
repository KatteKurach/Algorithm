#include <iostream>

using namespace std;
int a[10000];

void qsort(int l, int r)
{
    int middle = (r - l) / 2;
    int index1 = l;
    int index2 = r;

    while(true)
    {
        for(int i = index1; i < middle; i++)
        {
            if (a[i] > middle)
            {
                index1 = i;
                break;
            }
        }
        for (int i = index2; i > middle; i--)
        {
            if (a[i] < middle)
            {
                swap(a[index1], a[i]);
                index2 = i;
                break;
            }
        }
        if (index1 > index2)
        {
            break;
        }
    }
    if(middle - l > 1)
    {
        qsort(l, middle);
    }
    if (r - middle > 1)
    {
        qsort(middle, r);
    }
}

int main()
{
    freopen("input", "r", stdin);
    int n;
    cin >> n;

    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    
    qsort(0, n);
    for (int i = 0; i < n; i++)
    {
        cout << a[i] << " ";
    }
}

